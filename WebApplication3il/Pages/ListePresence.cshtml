@page
@model WebApplication3il.Pages.ListePresenceModel

@using System.Data
@using Api3il.Models;


@{
    var ListeEtudiant = ViewData["Etudiant"] as List<Etudiant>;

}

<center><h2>liste des etudiants qui ont emarge </h2></center> 


<table class="table table-sm table-striped table-bordered m-2">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nom</th>
            <th>Prénom</th>
            <th>Groupe</th>
            <th>promotion</th>

            
        </tr>
    </thead>
    <tbody>
        @if (ListeEtudiant != null)
       {
          @foreach (Etudiant row in ListeEtudiant)
        {
            <tr>
                <td>@row.Nom</td>
                <td>@row.Prenom</td>
                 
               
            </tr>
        }
       }
